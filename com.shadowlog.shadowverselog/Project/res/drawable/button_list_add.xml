<?xml version="1.0" encoding="utf-8"?>
<selector>
    <item n1:state_pressed="false"
      xmlns:n1="http://schemas.android.com/apk/res/android">
        <shape n1:shape="rectangle">
            <stroke n1:width="2.0dip" n1:color="#ffb0b0b0" n1:dashWidth="3.0dip" n1:dashGap="3.0dip" />
        </shape>
    </item>
    <item n2:state_pressed="true"
      xmlns:n2="http://schemas.android.com/apk/res/android">
        <shape n2:shape="rectangle">
            <stroke n2:width="2.0dip" n2:color="#ffb0b0b0" n2:dashWidth="3.0dip" n2:dashGap="3.0dip" />
            <solid n2:color="@color/list_select" />
        </shape>
    </item>
</selector>