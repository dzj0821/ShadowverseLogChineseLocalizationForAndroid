<?xml version="1.0" encoding="utf-8"?>
<selector>
    <item n1:state_pressed="true"
      xmlns:n1="http://schemas.android.com/apk/res/android">
        <shape n1:shape="rectangle">
            <solid n1:color="#ff0067b0" />
        </shape>
    </item>
    <item n2:state_pressed="false"
      xmlns:n2="http://schemas.android.com/apk/res/android">
        <layer-list>
            <item>
                <shape>
                    <solid n2:color="#ff0057a0" />
                </shape>
            </item>
            <item n2:left="1.0dip" n2:top="1.0dip" n2:right="1.0dip" n2:bottom="1.0dip">
                <shape>
                    <solid n2:color="#ff004790" />
                </shape>
            </item>
        </layer-list>
    </item>
</selector>