<?xml version="1.0" encoding="utf-8"?>
<selector>
    <item n1:state_pressed="true" n1:shape="rectangle"
      xmlns:n1="http://schemas.android.com/apk/res/android">
        <shape n1:shape="rectangle">
            <corners n1:radius="2.0dip" />
            <stroke n1:width="1.0dip" n1:color="#ff505050" />
            <solid n1:color="#ff16a085" />
        </shape>
        <color n1:color="@color/white" />
    </item>
    <item n2:shape="rectangle"
      xmlns:n2="http://schemas.android.com/apk/res/android">
        <shape n2:shape="rectangle">
            <corners n2:radius="2.0dip" />
            <stroke n2:width="1.0dip" n2:color="#ff909090" />
            <gradient n2:startColor="#ff404040" n2:endColor="#ff202020" />
        </shape>
        <color n2:color="@color/white" />
    </item>
</selector>