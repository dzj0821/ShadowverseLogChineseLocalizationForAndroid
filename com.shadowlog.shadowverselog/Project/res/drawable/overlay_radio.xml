<?xml version="1.0" encoding="utf-8"?>
<selector>
    <item n1:state_checked="true" n1:shape="rectangle"
      xmlns:n1="http://schemas.android.com/apk/res/android">
        <shape n1:shape="rectangle">
            <stroke n1:width="1.0dip" n1:color="#ff333333" />
            <solid n1:color="#ff16a085" />
        </shape>
        <color n1:color="@color/white" />
    </item>
    <item n2:state_checked="false" n2:shape="rectangle"
      xmlns:n2="http://schemas.android.com/apk/res/android">
        <shape n2:shape="rectangle">
            <stroke n2:width="1.0dip" n2:color="#ff333333" />
            <solid n2:color="#ff242424" />
        </shape>
        <color n2:color="@color/white" />
    </item>
</selector>